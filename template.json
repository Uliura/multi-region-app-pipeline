{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "profiles_frontdorname_name": {
            "defaultValue": "frontdorname",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Cdn/profiles",
            "apiVersion": "2021-06-01",
            "name": "[parameters('profiles_frontdorname_name')]",
            "location": "Global",
            "sku": {
                "name": "Standard_AzureFrontDoor"
            },
            "kind": "frontdoor",
            "properties": {
                "originResponseTimeoutSeconds": 60
            }
        },
        {
            "type": "Microsoft.Cdn/profiles/afdendpoints",
            "apiVersion": "2021-06-01",
            "name": "[concat(parameters('profiles_frontdorname_name'), '/frontend')]",
            "location": "Global",
            "dependsOn": [
                "[resourceId('Microsoft.Cdn/profiles', parameters('profiles_frontdorname_name'))]"
            ],
            "properties": {
                "enabledState": "Enabled"
            }
        },
        {
            "type": "Microsoft.Cdn/profiles/origingroups",
            "apiVersion": "2021-06-01",
            "name": "[concat(parameters('profiles_frontdorname_name'), '/origingroup')]",
            "dependsOn": [
                "[resourceId('Microsoft.Cdn/profiles', parameters('profiles_frontdorname_name'))]"
            ],
            "properties": {
                "loadBalancingSettings": {
                    "sampleSize": 4,
                    "successfulSamplesRequired": 3,
                    "additionalLatencyInMilliseconds": 50
                },
                "healthProbeSettings": {
                    "probePath": "/",
                    "probeRequestType": "HEAD",
                    "probeProtocol": "Http",
                    "probeIntervalInSeconds": 100
                },
                "sessionAffinityState": "Disabled"
            }
        },
        {
            "type": "Microsoft.Cdn/profiles/origingroups/origins",
            "apiVersion": "2021-06-01",
            "name": "[concat(parameters('profiles_frontdorname_name'), '/origingroup/first')]",
            "dependsOn": [
                "[resourceId('Microsoft.Cdn/profiles/origingroups', parameters('profiles_frontdorname_name'), 'origingroup')]",
                "[resourceId('Microsoft.Cdn/profiles', parameters('profiles_frontdorname_name'))]"
            ],
            "properties": {
                "hostName": "gekamultiappcus.azurewebsites.net",
                "httpPort": 80,
                "httpsPort": 443,
                "originHostHeader": "gekamultiappcus.azurewebsites.net",
                "priority": 1,
                "weight": 1000,
                "enabledState": "Enabled",
                "enforceCertificateNameCheck": true
            }
        },
        {
            "type": "Microsoft.Cdn/profiles/origingroups/origins",
            "apiVersion": "2021-06-01",
            "name": "[concat(parameters('profiles_frontdorname_name'), '/origingroup/second')]",
            "dependsOn": [
                "[resourceId('Microsoft.Cdn/profiles/origingroups', parameters('profiles_frontdorname_name'), 'origingroup')]",
                "[resourceId('Microsoft.Cdn/profiles', parameters('profiles_frontdorname_name'))]"
            ],
            "properties": {
                "hostName": "gekamultiappeus.azurewebsites.net",
                "httpPort": 80,
                "httpsPort": 443,
                "originHostHeader": "gekamultiappeus.azurewebsites.net",
                "priority": 1,
                "weight": 1000,
                "enabledState": "Enabled",
                "enforceCertificateNameCheck": true
            }
        },
        {
            "type": "Microsoft.Cdn/profiles/afdendpoints/routes",
            "apiVersion": "2021-06-01",
            "name": "[concat(parameters('profiles_frontdorname_name'), '/frontend/routerule')]",
            "dependsOn": [
                "[resourceId('Microsoft.Cdn/profiles/afdendpoints', parameters('profiles_frontdorname_name'), 'frontend')]",
                "[resourceId('Microsoft.Cdn/profiles', parameters('profiles_frontdorname_name'))]",
                "[resourceId('Microsoft.Cdn/profiles/origingroups', parameters('profiles_frontdorname_name'), 'origingroup')]"
            ],
            "properties": {
                "customDomains": [],
                "originGroup": {
                    "id": "[resourceId('Microsoft.Cdn/profiles/origingroups', parameters('profiles_frontdorname_name'), 'origingroup')]"
                },
                "ruleSets": [],
                "supportedProtocols": [
                    "Http",
                    "Https"
                ],
                "patternsToMatch": [
                    "/*"
                ],
                "forwardingProtocol": "MatchRequest",
                "linkToDefaultDomain": "Enabled",
                "httpsRedirect": "Enabled",
                "enabledState": "Enabled"
            }
        }
    ]
}